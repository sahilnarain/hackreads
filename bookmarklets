#1 Open all links on a page, if no links then click on NextPage
javascript:javascript:var x=document.querySelectorAll('a[class="gr-button"]');buttons=Array.prototype.slice.call(x,0);if(!buttons.length){var y = document.querySelectorAll('a[class="next_page"]');y[0].click();};buttons.forEach(function(node){window.open(node);});

#2 Address
javascript:var%20x=document.querySelector('a[id^="addressSelect"]').click();

#3 T&C and submit
javascript:document.getElementById('termsCheckBox').click();document.getElementById('want_to_read').click();document.querySelector('input[type=submit]').click();document.querySelector('input[type="submit"][value="Enter Giveaway"]').click();

#4 Close tab
javascript:setTimeout(function(){window.close();},300);
